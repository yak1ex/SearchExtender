{
  "name": "SearchExtender",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Search extender for omnibox and contextmenu",
  "author": "yak_ex",
  "omnibox": {
    "keyword" : "go"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "permissions": [ "contextMenus", "storage", "activeTab" ],
  "optional_permissions": [ "clipboardRead" ]
}
