{
  "name": "SearchExtender",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Search extender for omnibox and contextmenu",
  "author": "yak_ex",
  "omnibox": {
    "keyword" : "go"
  },
  "background": {
    "persistent": false,
    "scripts": ["multiua.js","background.js"]
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "permissions": [ "contextMenus", "storage", "activeTab" ],
  "optional_permissions": [ "clipboardRead", "clipboardWrite" ],
  "applications": {
    "gecko": {
      "id": "SearchExtender@yak1ex.github.io"
    }
  }
}
